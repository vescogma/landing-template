{
  "name": "landing-template",
  "version": "1.0.0",
  "description": "does things sometimes",
  "main": "index.js",
  "scripts": {
    "start": "npm run app:prod && npm run ssr && npm run serve:local",
    "app": "cross-env NODE_ENV=development webpack-dev-server --env=dev",
    "app:build:dev": "cross-env NODE_ENV=development webpack --env=dev --progress --profile --colors",
    "app:build:prod": "cross-env NODE_ENV=production MIN=false webpack --env=prod --progress --profile --colors",
    "app:prod": "cross-env NODE_ENV=production MIN=true webpack --env=prod --progress --profile --colors",
    "app:aws": "webpack --env=prod --progress --profile --colors",
    "aws": "npm run app:aws && npm run ssr && npm run serve",
    "server": "cross-env NODE_ENV=development nodemon ./server --exec babel-node",
    "server:debug": "cross-env NODE_ENV=development nodemon --inspect ./server --exec babel-node",
    "ssr:server": "cross-env NODE_ENV=production babel ./server -d ./dist/server",
    "ssr:app": "cross-env NODE_ENV=production babel ./src -d ./dist/src",
    "ssr": "npm run ssr:server && npm run ssr:app",
    "serve:local": "cross-env NODE_ENV=production node -r dotenv/config ./dist/server",
    "serve": "cross-env NODE_ENV=production node ./dist/server",
    "analyze": "cross-env NODE_ENV=production MIN=true webpack --env=prod --json > stats.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vescogma/front-end-test-05.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vescogma/front-end-test-05/issues"
  },
  "homepage": "https://github.com/vescogma/front-end-test-05#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-css-modules-transform": "^1.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "dotenv": "^4.0.0",
    "koa": "^2.3.0",
    "koa-router": "^7.2.1",
    "koa-socket": "^4.4.0",
    "koa-static-cache": "^5.1.1",
    "mailgun-js": "^0.13.1",
    "react-dom": "^15.6.1",
    "style-loader": "^0.18.2"
  },
  "devDependencies": {
    "compression-webpack-plugin": "^1.0.0",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "history": "^4.6.3",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "nodemon": "^1.11.0",
    "react": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "reselect": "^3.0.1",
    "source-map-loader": "^0.2.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1"
  }
}
